                android:textSize="20sp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@+id/insel_Verzweigung"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/insel_Ben"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="30dp"
                tools:layout_conversion_absoluteWidth="200dp" />

            <ImageView
                android:id="@+id/piratenSchiff_Ben"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="156dp"
                android:contentDescription="@string/desc"
                android:src="@drawable/ic_piratenschiff"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.249"
                app:layout_constraintStart_toStartOf="@+id/insel_Ben"
                app:layout_constraintTop_toBottomOf="@+id/piratenSchiff_Basics" />

            <ImageButton
                android:id="@+id/insel_Verzweigung"
