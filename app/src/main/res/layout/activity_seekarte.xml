        android:layout_height="match_parent"
        android:layout_marginTop="50dp">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toTopOf="@+id/Benutzerinteraktion_Themen"
            app:layout_constraintTop_toTopOf="@+id/Benutzerinteraktion_Themen">


            <ImageButton
                android:id="@+id/insel_grundlagen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="39dp"
                android:background="@null"
                android:contentDescription="@string/desc"
                android:src="@drawable/ic_inselpalme3"
                android:visibility="visible"
                app:layout_constraintBottom_toTopOf="@+id/insel_Variablen"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.193"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0"
                tools:layout_conversion_absoluteHeight="101dp"
                tools:layout_conversion_absoluteWidth="107dp" />

            <TextView
                android:id="@+id/basics_Themen"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:text="@string/grundlagen"
                android:textSize="20sp"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.16"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/insel_grundlagen"
                app:layout_goneMarginStart="56dp"
                tools:layout_conversion_absoluteHeight="0dp"
                tools:layout_conversion_absoluteWidth="0dp" />

            <ImageView
                android:id="@+id/piratenSchiff_Basics"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="56dp"
                android:layout_marginLeft="56dp"
                android:layout_marginBottom="136dp"
                android:contentDescription="@string/desc"
                android:src="@drawable/ic_piratenschiff"
                app:layout_constraintBottom_toTopOf="@+id/insel_Ben"
                app:layout_constraintLeft_toLeftOf="@+id/insel_grundlagen"
                tools:layout_conversion_absoluteHeight="68dp"
                tools:layout_conversion_absoluteWidth="144dp" />

            <ImageButton
                android:id="@+id/insel_Variablen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:layout_marginRight="16dp"
                android:background="@null"
                android:contentDescription="@string/desc"
                android:src="@drawable/ic_inselpalme3"
                app:layout_constraintBottom_toTopOf="@+id/variablen_Themen"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.857"
                tools:layout_conversion_absoluteHeight="100dp"
                tools:layout_conversion_absoluteWidth="100dp" />

            <TextView
                android:id="@+id/variablen_Themen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="127dp"
                android:layout_marginLeft="127dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:text="@string/variablen"
