            <ImageButton
                android:id="@+id/insel_Funktionen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="256dp"
                android:background="@null"
                android:contentDescription="@string/desc"
                android:src="@drawable/ic_inselpalme3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.193"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/insel_Schleifen"
                tools:layout_conversion_absoluteHeight="100dp"
                tools:layout_conversion_absoluteWidth="100dp" />

            <ImageView
                android:id="@+id/piratenSchiff_Funktionen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="68dp"
                android:layout_marginLeft="68dp"
                android:layout_marginTop="276dp"
                android:contentDescription="@string/desc"
                android:src="@drawable/ic_piratenschiff"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.035"
                app:layout_constraintStart_toStartOf="@+id/insel_Funktionen"
                app:layout_constraintTop_toTopOf="@+id/insel_Schleifen" />

            <TextView
                android:id="@+id/Funktionen_Themen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="43dp"
                android:text="@string/funktionen"
                android:textSize="20sp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.116"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/insel_Funktionen"
                app:layout_constraintVertical_bias="0.259"
                tools:layout_conversion_absoluteHeight="30dp"
                tools:layout_conversion_absoluteWidth="200dp" />

            <ImageButton
                android:id="@+id/insel_Zeiger"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="128dp"
                android:background="@null"
                android:contentDescription="@string/desc"
                android:src="@drawable/ic_inselpalme3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.727"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/insel_Funktionen"
                tools:layout_conversion_absoluteHeight="100dp"
                tools:layout_conversion_absoluteWidth="100dp" />

            <ImageView
                android:id="@+id/piratenSchiff_Zeiger"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="148dp"
                android:contentDescription="@string/desc"
                android:src="@drawable/ic_piratenschiff"
                app:layout_constraintStart_toStartOf="@+id/insel_Zeiger"
